{
  "name": "compute-starter-kit-typescript",
  "version": "0.2.0",
  "type": "module",
  "scripts": {
    "clean": "rimraf bin build dist pkg target",
    "bundle-client": "npx -y esbuild --bundle --platform=browser --format=esm --outdir=./dist ./client/index.tsx",
    "bundle-client-watch": "npm run bundle-client -- --watch --servedir=.",
    "bundle-client-production": "npm run bundle-client -- --minify --sourcemap",
    "prebuild": "npm run bundle-client-production && tsc",
    "build": "js-compute-runtime build/index.js bin/main.wasm",
    "serve": "fastly compute serve --watch",
    "deploy": "fastly compute publish"
  },
  "engines": {
    "node": "16 || >=18"
  },
  "dependencies": {
    "@fluentui/react-components": "9.29.4",
    "@fluentui/react-icons-mdl2": "1.3.49",
    "@hpcc-js/observablehq-compiler": "1.2.6",
    "@observablehq/runtime": "5.8.2",
    "@octokit/core": "4.2.4",
    "langchain": "0.0.109",
    "typeorm": "0.3.17"
  },
  "devDependencies": {
    "@fastly/js-compute": "2.5.0",
    "esbuild": "0.18.17",
    "esbuild-dev-server": "0.3.0",
    "esbuild-serve": "1.0.1",
    "npm-run-all": "4.1.5",
    "rimraf": "5.0.1",
    "typescript": "5.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fastly/compute-starter-kit-typescript.git"
  },
  "author": "oss@fastly.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fastly/compute-starter-kit-typescript/issues"
  },
  "homepage": "https://developer.fastly.com/solutions/starters/compute-starter-kit-typescript"
}